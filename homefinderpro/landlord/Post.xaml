<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:maps="clr-namespace:Microsoft.Maui.Controls.Maps;assembly=Microsoft.Maui.Controls.Maps"
             x:Class="homefinderpro.landlord.Post"
             xmlns:local="clr-namespace:homefinderpro.LandlordViewModels"
             Title="Post">

    <StackLayout>
        <Entry Placeholder="Apartment Name" x:Name="ApartmentNameEntry" />
        <Entry Placeholder="Price (PHP)" x:Name="PriceEntry" />
        <Entry Placeholder="Descriptions" x:Name="DescriptionsEntry" />
        <Entry Placeholder="Locations" x:Name="LocationEntry" />
    </StackLayout>
    
    

</ContentPage>